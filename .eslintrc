{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"parser": "babel-eslint",
	"plugins": [
		"react"
	]
}
