{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"exclude": [
			"node_modules",
			"client/node_modules",
			"client/build"
		]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}

